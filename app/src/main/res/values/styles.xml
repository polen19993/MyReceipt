<resources xmlns:android="http://schemas.android.com/apk/res/android">


    <style name="TextAppearance.Regular" parent="android:TextAppearance">
        <item name="android:fontFamily">sans-serif</item>
    </style>

    <style name="TextAppearance.Medium" parent="android:TextAppearance">
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="CustomAlertDialogStyle" parent="android:Theme.Holo.Light.Dialog">
        <item name="android:background">@drawable/bg_custom_progress</item>
        <item name="android:windowBackground">@color/transparent</item>
    </style>

    <!-- piao-->

    <style name="SearchViewStyle" parent="@style/Widget.AppCompat.Light.SearchView">
        <item name="suggestionRowLayout">@layout/search_dropdown_item_icons_2line</item>
    </style>

    <style name="SearchDropDownTheme" parent="@style/RtlOverlay.Widget.AppCompat.Search.DropDown">
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/white</item>
        <!-- <item name="colorAccent">@color/primary</item> -->
        <!-- <item name="android:textColorPrimary">@color/primary_dark</item> -->
    </style>

</resources>
